{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": ".*\\.md:[0-9]+"
    },
    {
      "pattern": ".*\\.md:[0-9]+-[0-9]+"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://huggingface.co/"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "5s",
  "aliveStatusCodes": [200, 206, 301, 302, 307, 308, 403, 429]
}
