{
  "dashboard": {
    "id": null,
    "uid": "aegis-rag-performance",
    "title": "AegisRAG Performance Monitoring",
    "tags": ["aegis-rag", "performance", "sprint-28"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "liveNow": false,
    "panels": [
      {
        "id": 1,
        "title": "Request Latency (p50, p95, p99)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(aegis_rag_request_latency_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p50 - {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(aegis_rag_request_latency_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p95 - {{endpoint}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(aegis_rag_request_latency_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p99 - {{endpoint}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1.0}
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max"]
          }
        }
      },
      {
        "id": 2,
        "title": "Throughput (Requests per Second)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_rag_requests_total[1m])) by (endpoint)",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate (%)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_rag_requests_total{status=~\"4..|5..\"}[1m])) by (endpoint) / sum(rate(aegis_rag_requests_total[1m])) by (endpoint) * 100",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 30,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "max": 10
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 4,
        "title": "Memory Usage (RSS)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"aegis-rag-api\"}",
            "legendFormat": "API Server RSS",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1610612736},
                {"color": "red", "value": 2147483648}
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 5,
        "title": "Database Connection Pool Usage",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "qdrant_connections_active / qdrant_connections_max * 100",
            "legendFormat": "Qdrant Pool Usage %",
            "refId": "A"
          },
          {
            "expr": "neo4j_connections_active / neo4j_connections_max * 100",
            "legendFormat": "Neo4j Pool Usage %",
            "refId": "B"
          },
          {
            "expr": "redis_connections_active / redis_connections_max * 100",
            "legendFormat": "Redis Pool Usage %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 15,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "orange", "value": 85},
                {"color": "red", "value": 95}
              ]
            },
            "max": 100
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 6,
        "title": "LLM Token Throughput",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(llm_tokens_generated_total[1m])",
            "legendFormat": "Tokens/Second",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 7,
        "title": "Request Status Breakdown",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 24
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(increase(aegis_rag_requests_total[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          }
        }
      },
      {
        "id": 8,
        "title": "Endpoint Request Distribution",
        "type": "bargauge",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 24
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(increase(aegis_rag_requests_total[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        }
      },
      {
        "id": 9,
        "title": "Average Request Latency by Endpoint",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "avg(rate(aegis_rag_request_latency_seconds_sum[5m])) by (endpoint) / avg(rate(aegis_rag_request_latency_seconds_count[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.2},
                {"color": "orange", "value": 0.5},
                {"color": "red", "value": 1.0}
              ]
            },
            "decimals": 3
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 10,
        "title": "System Health Overview",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "up{job=\"aegis-rag-api\"}",
            "legendFormat": "API Server",
            "refId": "A",
            "instant": true
          },
          {
            "expr": "up{job=\"qdrant\"}",
            "legendFormat": "Qdrant",
            "refId": "B",
            "instant": true
          },
          {
            "expr": "up{job=\"neo4j\"}",
            "legendFormat": "Neo4j",
            "refId": "C",
            "instant": true
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis",
            "refId": "D",
            "instant": true
          },
          {
            "expr": "up{job=\"ollama\"}",
            "legendFormat": "Ollama",
            "refId": "E",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                }
              }
            ]
          }
        },
        "options": {
          "showHeader": true,
          "sortBy": []
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "Value": "Status"
              }
            }
          }
        ]
      },
      {
        "id": 11,
        "title": "Cache Hit Rate (Redis)",
        "type": "gauge",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 40
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 12,
        "title": "Qdrant Vector Count",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 40
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "qdrant_collections_vectors_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50000},
                {"color": "red", "value": 100000}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 13,
        "title": "Neo4j Graph Size (Nodes + Relationships)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 40
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "neo4j_node_count + neo4j_relationship_count",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10000},
                {"color": "red", "value": 50000}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "textMode": "value_and_name"
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "endpoint",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(aegis_rag_requests_total, endpoint)",
          "refresh": 1,
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    }
  },
  "overwrite": true,
  "message": "AegisRAG Performance Dashboard - Sprint 28 Feature 28.4",
  "folderUid": "aegis-rag",
  "inputs": [
    {
      "name": "DS_PROMETHEUS",
      "type": "datasource",
      "pluginId": "prometheus",
      "value": "Prometheus"
    }
  ]
}
