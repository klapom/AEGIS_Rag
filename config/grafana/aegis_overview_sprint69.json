{
  "dashboard": {
    "id": null,
    "uid": "aegis-sprint69-overview",
    "title": "AegisRAG Production Overview (Sprint 69)",
    "tags": ["aegis-rag", "sprint-69", "production", "observability"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "Query Rate (QPS)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_queries_total[1m]))",
            "legendFormat": "Total QPS",
            "refId": "A"
          },
          {
            "expr": "sum(rate(aegis_queries_total[1m])) by (intent)",
            "legendFormat": "{{intent}}",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull", "mean"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 2,
        "title": "P95 Latency by Stage",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(aegis_query_latency_seconds_bucket[5m])) by (le, stage))",
            "legendFormat": "{{stage}} (p95)",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull", "max"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "yellow"
                },
                {
                  "value": 5,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_cache_hits_total[5m])) / (sum(rate(aegis_cache_hits_total[5m])) + sum(rate(aegis_cache_misses_total[5m])))",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 0.5,
                  "color": "yellow"
                },
                {
                  "value": 0.8,
                  "color": "green"
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "gauge",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_errors_total[5m])) / sum(rate(aegis_queries_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 0.3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 0.05,
                  "color": "yellow"
                },
                {
                  "value": 0.2,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Memory Facts Count",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(aegis_memory_facts_count)",
            "legendFormat": "Total Facts",
            "refId": "A"
          },
          {
            "expr": "aegis_memory_facts_count{fact_type=\"episodic\"}",
            "legendFormat": "Episodic",
            "refId": "B"
          },
          {
            "expr": "aegis_memory_facts_count{fact_type=\"semantic\"}",
            "legendFormat": "Semantic",
            "refId": "C"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "LLM Requests & Tokens",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(llm_requests_total[5m]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(llm_tokens_used[5m]))",
            "legendFormat": "Tokens/sec",
            "refId": "B"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 7,
        "title": "Query Latency Distribution (Heatmap)",
        "type": "heatmap",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_query_latency_seconds_bucket{stage=\"total\"}[5m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": true,
          "color": {
            "mode": "scheme",
            "scheme": "Spectral"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 8,
        "title": "Errors by Type",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_errors_total[5m])) by (error_type)",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull", "sum"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Queries by Intent & Model",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 25
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(increase(aegis_queries_total[1h])) by (intent)",
            "legendFormat": "{{intent}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "title": "Cache Performance by Type",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 25
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_cache_hits_total[5m])) by (cache_type)",
            "legendFormat": "{{cache_type}} hits",
            "refId": "A"
          },
          {
            "expr": "sum(rate(aegis_cache_misses_total[5m])) by (cache_type)",
            "legendFormat": "{{cache_type}} misses",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull", "mean"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 11,
        "title": "Database Metrics",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 25
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "qdrant_points_count{collection=\"documents\"}",
            "legendFormat": "Qdrant Points",
            "refId": "A"
          },
          {
            "expr": "neo4j_entities_count",
            "legendFormat": "Neo4j Entities",
            "refId": "B"
          },
          {
            "expr": "neo4j_relations_count",
            "legendFormat": "Neo4j Relations",
            "refId": "C"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "vertical"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 12,
        "title": "LLM Cost (Monthly)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 33
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(monthly_spend_usd) by (provider)",
            "legendFormat": "{{provider}} spend",
            "refId": "A"
          },
          {
            "expr": "sum(monthly_budget_remaining_usd) by (provider)",
            "legendFormat": "{{provider}} remaining",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 13,
        "title": "LLM Latency by Provider",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 33
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(llm_latency_seconds_bucket[5m])) by (le, provider))",
            "legendFormat": "{{provider}} (p95)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(llm_latency_seconds_bucket[5m])) by (le, provider))",
            "legendFormat": "{{provider}} (p50)",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull", "mean"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 14,
        "title": "Service Availability",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 41
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "up{job=\"aegis-api\"}",
            "legendFormat": "API",
            "refId": "A"
          },
          {
            "expr": "up{job=\"qdrant\"}",
            "legendFormat": "Qdrant",
            "refId": "B"
          },
          {
            "expr": "up{job=\"neo4j\"}",
            "legendFormat": "Neo4j",
            "refId": "C"
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis",
            "refId": "D"
          },
          {
            "expr": "up{job=\"prometheus\"}",
            "legendFormat": "Prometheus",
            "refId": "E"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 1,
                  "color": "green"
                }
              ]
            }
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    }
  }
}
