                {"color": "orange", "value": 75},
                {"color": "red", "value": 100}
              ]
            },
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "orientation": "auto",
          "showThresholdLabels": false,
          "showThresholdMarkers": true,
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
