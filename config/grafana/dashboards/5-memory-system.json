      {
        "id": 5,
        "title": "Layer Capacity Usage %",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "(graphiti_facts_total{layer=\"episodic\"} / 1000000) * 100",
            "legendFormat": "Episodic (1M max)",
            "refId": "A"
          },
          {
            "expr": "(graphiti_facts_total{layer=\"semantic\"} / 500000) * 100",
            "legendFormat": "Semantic (500K max)",
            "refId": "B"
          },
          {
            "expr": "(graphiti_facts_total{layer=\"procedural\"} / 100000) * 100",
            "legendFormat": "Procedural (100K max)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 15,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            },
            "max": 100
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 6,
        "title": "Read Latency by Layer (ms)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(graphiti_read_latency_seconds_bucket{layer=\"episodic\"}[5m])) by (le)) * 1000",
