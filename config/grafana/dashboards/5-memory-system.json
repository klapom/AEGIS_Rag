          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "graphiti_facts_total{type=\"entity\"}",
            "legendFormat": "Entity Facts",
            "refId": "A"
          },
          {
            "expr": "graphiti_facts_total{type=\"relationship\"}",
            "legendFormat": "Relationship Facts",
            "refId": "B"
