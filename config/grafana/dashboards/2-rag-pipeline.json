{
  "dashboard": {
    "id": null,
    "uid": "aegis-rag-pipeline",
    "title": "RAG Pipeline Performance",
    "tags": ["aegis-rag", "rag", "pipeline", "sprint-97"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
      "time_options": ["15m", "1h", "6h", "12h", "24h"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "liveNow": false,
    "panels": [
      {
        "id": 1,
        "title": "Latency by Stage (ms)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(aegis_query_latency_seconds_bucket{stage=\"embedding\"}[5m])) by (le)) * 1000",
            "legendFormat": "P95 Embedding (ms)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(aegis_query_latency_seconds_bucket{stage=\"vector_search\"}[5m])) by (le)) * 1000",
            "legendFormat": "P95 Vector Search (ms)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(aegis_query_latency_seconds_bucket{stage=\"graph_search\"}[5m])) by (le)) * 1000",
            "legendFormat": "P95 Graph Search (ms)",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(aegis_query_latency_seconds_bucket{stage=\"reranking\"}[5m])) by (le)) * 1000",
            "legendFormat": "P95 Reranking (ms)",
            "refId": "D"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(aegis_query_latency_seconds_bucket{stage=\"llm_generation\"}[5m])) by (le)) * 1000",
            "legendFormat": "P95 LLM Generation (ms)",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 500},
                {"color": "red", "value": 1500}
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 2,
        "title": "Intent Distribution",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_queries_total[5m])) by (intent)",
            "legendFormat": "{{intent}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [],
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              }
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "calcs": ["value", "percent"]
          }
        }
      },
      {
        "id": 3,
        "title": "Cache Hit Rate %",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "(redis_hits / (redis_hits + redis_misses)) * 100",
            "legendFormat": "Redis Hit Rate",
            "refId": "A"
          },
          {
            "expr": "(memory_cache_hits / (memory_cache_hits + memory_cache_misses)) * 100",
            "legendFormat": "Memory Cache Hit Rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 15,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 75}
              ]
            },
            "max": 100
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate by Type",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(aegis_errors_total{error_type=\"validation\"}[5m])) by (error_type)",
            "legendFormat": "Validation Errors",
            "refId": "A"
          },
          {
            "expr": "sum(rate(aegis_errors_total{error_type=\"retrieval\"}[5m])) by (error_type)",
            "legendFormat": "Retrieval Errors",
            "refId": "B"
          },
          {
            "expr": "sum(rate(aegis_errors_total{error_type=\"generation\"}[5m])) by (error_type)",
            "legendFormat": "Generation Errors",
            "refId": "C"
          },
          {
            "expr": "sum(rate(aegis_errors_total{error_type=\"timeout\"}[5m])) by (error_type)",
            "legendFormat": "Timeout Errors",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "errps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "sum", "last"]
          }
        }
      },
      {
        "id": 5,
        "title": "Query Success Rate %",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 16
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "(sum(rate(aegis_queries_total{status=\"success\"}[5m])) / sum(rate(aegis_queries_total[5m]))) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "mappings": []
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background",
          "justifyMode": "auto",
          "textMode": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 6,
        "title": "Average Context Recall",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 16
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "avg(ragas_context_recall)",
            "legendFormat": "Context Recall",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.8}
              ]
            },
            "mappings": [],
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background",
          "justifyMode": "auto",
          "textMode": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 7,
        "title": "Average Faithfulness",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 16
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "avg(ragas_faithfulness)",
            "legendFormat": "Faithfulness",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.85}
              ]
            },
            "mappings": [],
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background",
          "justifyMode": "auto",
          "textMode": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 8,
        "title": "Avg Context Precision",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 16
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "avg(ragas_context_precision)",
            "legendFormat": "Context Precision",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            },
            "mappings": [],
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background",
          "justifyMode": "auto",
          "textMode": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          }
        }
      }
    ]
  }
}
