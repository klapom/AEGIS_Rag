    "panels": [
      {
        "id": 1,
        "title": "Qdrant - Active Connections",
        "type": "timeseries",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 0
        },
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "qdrant_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "qdrant_connections_max",
            "legendFormat": "Max Connections",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 95}
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
