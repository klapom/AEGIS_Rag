{
  "dashboard": {
    "title": "AEGIS RAG - LLM Cost & Performance Monitoring",
    "description": "Monitor LLM usage, costs, and performance across multiple providers (Local Ollama, Alibaba Cloud, OpenAI)",
    "tags": ["aegis-rag", "llm", "cost-tracking", "sprint-24"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Total Requests (24h)",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(increase(llm_requests_total[24h]))",
            "legendFormat": "Total Requests",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1000, "color": "yellow" },
                { "value": 5000, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Total Cost (24h)",
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(increase(llm_cost_usd[24h]))",
            "legendFormat": "Total Cost",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 4,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1.0, "color": "yellow" },
                { "value": 10.0, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Total Tokens (24h)",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(increase(llm_tokens_used{token_type=\"total\"}[24h]))",
            "legendFormat": "Total Tokens",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1000000, "color": "yellow" },
                { "value": 10000000, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "P95 Latency (24h)",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(llm_latency_seconds_bucket[24h])) by (le))",
            "legendFormat": "P95 Latency",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 2,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1.0, "color": "yellow" },
                { "value": 5.0, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "Request Rate by Provider",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(llm_requests_total[5m])) by (provider)",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "showPoints": "never"
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "lastNotNull"]
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Cost by Provider (Hourly)",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(llm_cost_usd[1h])) by (provider)",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 4,
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 80,
              "showPoints": "never"
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["sum", "mean"]
          }
        }
      },
      {
        "id": 7,
        "type": "timeseries",
        "title": "Latency by Provider (P50, P95, P99)",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(llm_latency_seconds_bucket[5m])) by (provider, le))",
            "legendFormat": "{{provider}} - P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(llm_latency_seconds_bucket[5m])) by (provider, le))",
            "legendFormat": "{{provider}} - P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(llm_latency_seconds_bucket[5m])) by (provider, le))",
            "legendFormat": "{{provider}} - P99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 2,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "showPoints": "never"
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "lastNotNull"]
          }
        }
      },
      {
        "id": 8,
        "type": "timeseries",
        "title": "Token Usage by Provider",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(llm_tokens_used{token_type=\"total\"}[5m])) by (provider)",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "showPoints": "never"
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "lastNotNull"]
          }
        }
      },
      {
        "id": 9,
        "type": "piechart",
        "title": "Request Distribution by Task Type",
        "gridPos": {
          "x": 0,
          "y": 20,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(llm_requests_total[24h])) by (task_type)",
            "legendFormat": "{{task_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "pie"
        }
      },
      {
        "id": 10,
        "type": "piechart",
        "title": "Cost Distribution by Provider (24h)",
        "gridPos": {
          "x": 8,
          "y": 20,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(llm_cost_usd[24h])) by (provider)",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 4
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut"
        }
      },
      {
        "id": 11,
        "type": "gauge",
        "title": "Monthly Budget Status",
        "gridPos": {
          "x": 16,
          "y": 20,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "monthly_budget_remaining_usd{provider=\"alibaba_cloud\"}",
            "legendFormat": "Alibaba Cloud",
            "refId": "A"
          },
          {
            "expr": "monthly_budget_remaining_usd{provider=\"openai\"}",
            "legendFormat": "OpenAI",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2,
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 10, "color": "yellow" },
                { "value": 50, "color": "green" }
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 12,
        "type": "table",
        "title": "Error Summary by Provider",
        "gridPos": {
          "x": 0,
          "y": 28,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(llm_errors_total[24h])) by (provider, error_type)",
            "legendFormat": "{{provider}} - {{error_type}}",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Value",
              "desc": true
            }
          ]
        }
      },
      {
        "id": 13,
        "type": "timeseries",
        "title": "Monthly Spending Trend",
        "gridPos": {
          "x": 12,
          "y": 28,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "monthly_spend_usd",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "stepAfter",
              "fillOpacity": 20,
              "showPoints": "never"
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["lastNotNull", "max"]
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "provider",
          "type": "query",
          "label": "Provider",
          "query": "label_values(llm_requests_total, provider)",
          "multi": true,
          "includeAll": true,
          "refresh": 2
        },
        {
          "name": "task_type",
          "type": "query",
          "label": "Task Type",
          "query": "label_values(llm_requests_total, task_type)",
          "multi": true,
          "includeAll": true,
          "refresh": 2
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(aegis_rag_requests_total[5m]) > 0",
          "iconColor": "blue"
        }
      ]
    }
  }
}
